{
  "rulesDirectory": [],
  "extends": [
    "tslint:recommended",
    "rxjs-tslint-rules"
  ],
  "defaultSeverity": "error",
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts"
    ]
  },
  "rules": {
    "member-access": false,
    "no-empty": [true, "allow-empty-catch"],
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": [
      true,
      1
    ],
    "arrow-return-shorthand": true,
    "max-line-length": [
      true,
      140
    ],
    "no-inferrable-types": true,
    "class-name": true,
    "indent": [
      true,
      "spaces",
      2
    ],
    "eofline": true,
    "no-duplicate-variable": true,
    "no-eval": true,
    "no-arg": true,
    "no-internal-module": true,
    "no-trailing-whitespace": true,
    "space-before-function-paren": [
      true,
      {
        "anonymous": "never",
        "named": "never",
        "asyncArrow": "always",
        "method": "never",
        "constructor": "never"
      }
    ],
    "space-within-parens": false,
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "no-bitwise": true,
    "no-shadowed-variable": true,
    "no-unused-expression": true,
    "one-line": [
      true,
      "check-catch",
      "check-else",
      "check-open-brace",
      "check-whitespace",
      "check-finally"
    ],
    "quotemark": [
      true,
      "single",
      "avoid-escape",
      "jsx-double"
    ],
    "semicolon": [
      true,
      "always"
    ],
    "align": [
      true,
      "parameters",
      "statements",
      "members"
    ],
    "curly": true,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type",
      "check-module"
    ],
    "prefer-const": true,
    "no-duplicate-imports": true,
    "typedef": [
      true,
      "property-declaration"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      },
      {
        "call-signature": "onespace",
        "index-signature": "onespace",
        "parameter": "onespace",
        "property-declaration": "onespace",
        "variable-declaration": "onespace"
      }
    ],
    "no-sparse-arrays": true,
    "switch-default": true,
    "no-switch-case-fall-through": true,
    "no-unsafe-finally": true,
    "encoding": true,
    "callable-types": true,
    "comment-format": [
      false,
      "check-space",
      "check-uppercase"
    ],
    "forin": true,
    "import-blacklist": [
      true
    ],
    "import-spacing": true,
    "interface-over-type-literal": true,
    "label-position": true,
    "member-ordering": [
      true, {
        "order": [
          "public-static-field",
          "public-instance-field",
          "private-static-field",
          "private-instance-field",
          "public-constructor",
          "private-constructor",
          "public-instance-method",
          "protected-instance-method",
          "private-instance-method"
        ]
      }
    ],
    "no-construct": true,
    "no-console": {
      "severity": "warning",
      "options": [
        "debug",
        "info",
        "time",
        "timeEnd",
        "trace",
        "log",
        "error",
        "warn"
      ]
    },
    "no-debugger": true,
    "no-default-export": true,
    "no-empty-interface": true,
    "no-reference": true,
    "no-string-literal": false,
    "no-string-throw": true,
    "no-var-keyword": true,
    "object-literal-sort-keys": false,
    "radix": true,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "arrays": "ignore",
          "objects": "ignore",
          "functions": "ignore",
          "imports": "ignore",
          "exports": "ignore",
          "typeLiterals": "ignore"
        },
        "singleline": "never"
      }
    ],
    "triple-equals": false,
    "unified-signatures": true,
    "interface-name": [true, "never-prefix"],
    "ordered-imports": [true, {
      "import-sources-order": "any",
      "named-imports-order": "case-insensitive"
    }]
  }
}
